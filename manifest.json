{
  "name": "Permissions Onboarding",
  "version": "0.0.6",
  "description": "Open tab on install & ask for permission",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://support.google.com/*"],
      "js": ["contentA.js", "contentB.js"]
    }
  ],
  "permissions": ["scripting", "unlimitedStorage"],
  "optional_permissions": ["tabs", "topSites"],
  "host_permissions": ["<all_urls>"],
  "manifest_version": 3
}
